{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Rakulog 使い方マニュアル",
  "colors": {
    "primary": "#3C78C8",
    "light": "#72AFE7",
    "dark": "#2C5CA9"
  },
  "favicon": "/favicon.svg",
  "logo": {
    "light": "/logo/light.webp",
    "dark": "/logo/dark.webp"
  },
  "navbar": {
    "links": [
      {
        "label": "お問い合わせ",
        "href": "https://app.rakulog.ai/support"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Rakulogへ戻る",
      "href": "https://app.rakulog.ai/meetings"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/mintlify",
      "github": "https://github.com/mintlify",
      "linkedin": "https://linkedin.com/company/mintlify"
    }
  },
  "navigation": {
    "languages": [
      {
        "language": "jp",
        "tabs": [
          {
            "tab": "Guides",
            "groups": [
              {
                "group": "基本的な使い方",
                "pages": [
                  "jp/index",
                  "jp/quickstart",
                  "jp/development",
                  "jp/create-meeting-notes"
                ]
              },
              {
                "group": "Customization",
                "pages": [
                  "jp/essentials/settings",
                  "jp/essentials/navigation"
                ]
              },
              {
                "group": "Writing content",
                "pages": [
                  "jp/essentials/markdown",
                  "jp/essentials/code",
                  "jp/essentials/images",
                  "jp/essentials/reusable-snippets"
                ]
              },
              {
                "group": "AI tools",
                "pages": [
                  "jp/ai-tools/cursor",
                  "jp/ai-tools/claude-code",
                  "jp/ai-tools/windsurf"
                ]
              }
            ]
          },
          {
            "tab": "API reference",
            "groups": [
              {
                "group": "API documentation",
                "pages": [
                  "jp/api-reference/introduction"
                ]
              },
              {
                "group": "Endpoint examples",
                "pages": [
                  "jp/api-reference/endpoint/get",
                  "jp/api-reference/endpoint/create",
                  "jp/api-reference/endpoint/delete",
                  "jp/api-reference/endpoint/webhook"
                ]
              }
            ]
          }
        ],
        "global": {
          "anchors": [
            {
              "anchor": "Documentation",
              "href": "https://mintlify.com/docs",
              "icon": "book-open-cover"
            },
            {
              "anchor": "Blog",
              "href": "https://mintlify.com/blog",
              "icon": "newspaper"
            }
          ]
        }
      }
    ]
  },
  "redirects": [
    { "source": "/index", "destination": "/jp" },
    { "source": "/quickstart", "destination": "/jp/quickstart" },
    { "source": "/development", "destination": "/jp/development" },
    { "source": "/essentials/settings", "destination": "/jp/essentials/settings" },
    { "source": "/essentials/navigation", "destination": "/jp/essentials/navigation" },
    { "source": "/essentials/markdown", "destination": "/jp/essentials/markdown" },
    { "source": "/essentials/code", "destination": "/jp/essentials/code" },
    { "source": "/essentials/images", "destination": "/jp/essentials/images" },
    { "source": "/essentials/reusable-snippets", "destination": "/jp/essentials/reusable-snippets" },
    { "source": "/ai-tools/cursor", "destination": "/jp/ai-tools/cursor" },
    { "source": "/ai-tools/claude-code", "destination": "/jp/ai-tools/claude-code" },
    { "source": "/ai-tools/windsurf", "destination": "/jp/ai-tools/windsurf" },
    { "source": "/api-reference/introduction", "destination": "/jp/api-reference/introduction" },
    { "source": "/api-reference/endpoint/get", "destination": "/jp/api-reference/endpoint/get" },
    { "source": "/api-reference/endpoint/create", "destination": "/jp/api-reference/endpoint/create" },
    { "source": "/api-reference/endpoint/delete", "destination": "/jp/api-reference/endpoint/delete" },
    { "source": "/api-reference/endpoint/webhook", "destination": "/jp/api-reference/endpoint/webhook" }
  ]
}
